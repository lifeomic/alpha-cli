    "version": "0.2.0",
    "configurations": [        
        {
            "type": "node",
            "request": "launch",
            "name": "Forum Service Proxy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/cli.js",
            "args": ["--proxy", "--proxy-port", "9000", "lambda://forum-service:deployed"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Review Service Proxy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/cli.js",
            "args": ["--proxy", "--proxy-port", "9001", "lambda://review-service:deployed"]
        },
    ]
}